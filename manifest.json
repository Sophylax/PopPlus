{
  "name": "PopPlus",
  "description": "A Browser Extension including Quality of Life and Micro-Automation scripts for Popmundo",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "background/lib/utils.js",
      "background/lib/request.js",
      "background/initalize.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage",
    "https://*.popmundo.com/*",
    "https://popmundo.com/*"
  ]
}